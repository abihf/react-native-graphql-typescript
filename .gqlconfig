{
  "schema": {
    "files": "schema.gql"
  },
  "query": {
    "files": [ /* define file paths which you'd like the gql parser to watch and give autocomplete suggestions for */
      {
        "match": "src/**/*.tsx",
        "parser": ["EmbeddedQueryParser", { "startTag": "gql`", "endTag": "`" }],
        "isRelay": true,
      },
    ],
  },
}
